{
  "SUMMARY": "Package of environment files for SDK",
  "LICENSE": "MIT",
  "PR": "r8",
  "EXCLUDE_FROM_WORLD": "1",
  "MODIFYTOS": "0",
  "REAL_MULTIMACH_TARGET_SYS": "${TUNE_PKGARCH}${TARGET_VENDOR}-${TARGET_OS}",
  "inherit": "toolchain-scripts\ncross-canadian",
  "TOOLCHAIN_NEED_CONFIGSITE_CACHE": " zlib",
  "TOOLCHAIN_CONFIGSITE_NOCACHE": "${TOOLCHAIN_CONFIGSITE_NOCACHE}",
  "SDK_DIR": "${WORKDIR}/sdk",
  "SDK_OUTPUT": "${SDK_DIR}/image",
  "SDKTARGETSYSROOT": "${SDKPATH}/sysroots/${REAL_MULTIMACH_TARGET_SYS}",
  "PN": "meta-environment-${MACHINE}\nmeta-environment-extsdk-${MACHINE}",
  "PACKAGES": "${PN}",
  "FILES_${PN}": " \u001b    ${SDKPATH}/* \u001b    "
}