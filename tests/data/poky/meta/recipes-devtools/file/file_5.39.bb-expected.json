{
  "SUMMARY": "File classification tool",
  "DESCRIPTION": "File attempts to classify files depending \u001bon their contents and prints a description if a match is found.",
  "HOMEPAGE": "http://www.darwinsys.com/file/",
  "SECTION": "console/utils",
  "LICENSE": "BSD-2-Clause",
  "LIC_FILES_CHKSUM": "file://COPYING;beginline=2;md5=0251eaec1188b20d9a72c502ecfdda1b",
  "DEPENDS": "file-replacement-native\nbzip2-replacement-native",
  "SRC_URI": "git://github.com/file/file.git",
  "SRCREV": "87731415de945660b00f02207d8e9d986ef9b82e",
  "S": "${WORKDIR}/git",
  "inherit": "autotools update-alternatives",
  "PACKAGECONFIG": "zlib",
  "PACKAGECONFIG[bz2]": "--enable-bzlib, --disable-bzlib, bzip2",
  "PACKAGECONFIG[lzma]": "--enable-xzlib, --disable-xzlib, xz",
  "PACKAGECONFIG[zlib]": "--enable-zlib, --disable-zlib, zlib",
  "EXTRA_OECONF": "--disable-libseccomp",
  "ALTERNATIVE_${PN}": "file",
  "ALTERNATIVE_LINK_NAME[file]": "${bindir}/file",
  "EXTRA_OEMAKE": " -e FILE_COMPILE=${STAGING_BINDIR_NATIVE}/file-native/file\n -e FILE_COMPILE=${STAGING_BINDIR_NATIVE}/file-native/file",
  "FILES_${PN}": "${datadir}/misc/*.mgc",
  "BBCLASSEXTEND": "native nativesdk",
  "PROVIDES": " file-replacement-native",
  "bindir_append_class-native": "/file-native"
}