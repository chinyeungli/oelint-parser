  "acpaths": "-I .",
  "BBCLASSEXTEND": "native nativesdk",
  "FILES_${PN}-dev": "",
  "FILES_${PN}": "${datadir}/aclocal/pkg.m4",
  "RPROVIDES_${PN}": "pkgconfig(pkg-config)",
  "SSTATEPOSTUNPACKFUNCS": " pkgconfig_sstate_fixup_esdk"
}