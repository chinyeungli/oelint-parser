  "LIC_FILES_CHKSUM": "file://COPYING;md5=1ebbd3e34237af26da5dc08a4e440464",
  "PROVIDES": " texinfo-replacement-native",
  "RDEPENDS": "!!!inlineblock!!!",
  "DEPENDS": "zlib ncurses texinfo-replacement-native\nzlib-native ncurses-native",
  "TARGET_PATCH": "file://use_host_makedoc.patch\n",
  "SRC_URI": "${GNU_MIRROR}/texinfo/${BP}.tar.gz \u001b           file://texinfo-4.12-zlib.patch \u001b           file://disable-native-tools.patch \u001b           file://link-zip.patch \u001b           file://dont-depend-on-help2man.patch \u001b           ${TARGET_PATCH} \u001b           ",
  "SRC_URI[md5sum]": "f0c1782f68ef73738d74bd1e9e30793a",
  "SRC_URI[sha256sum]": "a52d05076b90032cb2523673c50e53185938746482cf3ca0213e9b4b50ac2d3e",
