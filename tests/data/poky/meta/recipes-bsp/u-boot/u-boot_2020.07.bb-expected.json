{
  "HOMEPAGE": "http://www.denx.de/wiki/U-Boot/WebHome",
  "SUMMARY": "Universal Boot Loader for embedded devices",
  "DESCRIPTION": "U-Boot, a boot loader for Embedded boards based on PowerPC, \u001bARM, MIPS and several other processors, which can be installed in a boot \u001bROM and used to initialize and test the hardware or to download and run \u001bapplication code.",
  "PROVIDES": "virtual/bootloader",
  "PACKAGE_ARCH": "${MACHINE_ARCH}",
  "SECTION": "bootloaders",
  "DEPENDS": "kern-tools-native\nflex-native bison-native\nswig-native\nbc-native dtc-native",
  "inherit": "uboot-config uboot-extlinux-config uboot-sign deploy cml1 python3native",
  "LICENSE": "GPLv2+",
  "LIC_FILES_CHKSUM": "file://Licenses/README;md5=30503fd321432fc713238f582193b78e",
  "PE": "1",
  "EXTRA_OEMAKE": "'CROSS_COMPILE=${TARGET_PREFIX} CC=\"${TARGET_PREFIX}gcc ${TOOLCHAIN_OPTIONS}\" V=1'\n'HOSTCC=\"${BUILD_CC} ${BUILD_CFLAGS} ${BUILD_LDFLAGS}\"'\n'STAGING_INCDIR=${STAGING_INCDIR_NATIVE} STAGING_LIBDIR=${STAGING_LIBDIR_NATIVE}'",
  "SRCREV": "2f5fbb5b39f7b67044dda5c35e4a4b31685a3109",
  "SRC_URI": "git://git.denx.de/u-boot.git \u001b          ",
  "PACKAGECONFIG": "openssl",
  "S": "${WORKDIR}/git",
  "B": "${WORKDIR}/build",
  "PACKAGECONFIG[openssl]": ",,openssl-native",
  "UBOOT_LOCALVERSION": "",
  "UBOOT_SUFFIX": "bin",
  "UBOOT_IMAGE": "u-boot-${MACHINE}-${PV}-${PR}.${UBOOT_SUFFIX}",
  "UBOOT_SYMLINK": "u-boot-${MACHINE}.${UBOOT_SUFFIX}",
  "UBOOT_MAKE_TARGET": "all",
  "UBOOT_ELF": "",
  "UBOOT_ELF_SUFFIX": "elf",
  "UBOOT_ELF_IMAGE": "u-boot-${MACHINE}-${PV}-${PR}.${UBOOT_ELF_SUFFIX}",
  "UBOOT_ELF_BINARY": "u-boot.${UBOOT_ELF_SUFFIX}",
  "UBOOT_ELF_SYMLINK": "u-boot-${MACHINE}.${UBOOT_ELF_SUFFIX}",
  "SPL_BINARY": "",
  "SPL_BINARYNAME": "!!!inlineblock!!!",
  "SPL_IMAGE": "${SPL_BINARYNAME}-${MACHINE}-${PV}-${PR}",
  "SPL_SYMLINK": "${SPL_BINARYNAME}-${MACHINE}",
  "UBOOT_ENV_SUFFIX": "txt",
  "UBOOT_ENV": "",
  "UBOOT_ENV_BINARY": "${UBOOT_ENV}.${UBOOT_ENV_SUFFIX}",
  "UBOOT_ENV_IMAGE": "${UBOOT_ENV}-${MACHINE}-${PV}-${PR}.${UBOOT_ENV_SUFFIX}",
  "UBOOT_ENV_SYMLINK": "${UBOOT_ENV}-${MACHINE}.${UBOOT_ENV_SUFFIX}",
  "UBOOT_INITIAL_ENV": "${PN}-initial-env",
  "UBOOT_EXTLINUX_INSTALL_DIR": "/boot/extlinux",
  "UBOOT_EXTLINUX_CONF_NAME": "extlinux.conf",
  "UBOOT_EXTLINUX_SYMLINK": "${UBOOT_EXTLINUX_CONF_NAME}-${MACHINE}-${PR}",
  "PACKAGE_BEFORE_PN": "${PN}-env",
  "RPROVIDES_${PN}-env": "u-boot-default-env",
  "ALLOW_EMPTY_${PN}-env": "1",
  "FILES_${PN}-env": " \u001b    !!!inlineblock!!! \u001b    ${sysconfdir}/fw_env.config \u001b",
  "FILES_${PN}": "/boot ${datadir}",
  "RDEPENDS_${PN}": "${PN}-env"
}