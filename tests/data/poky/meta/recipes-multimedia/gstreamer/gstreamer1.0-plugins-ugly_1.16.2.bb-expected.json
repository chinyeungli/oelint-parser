  "FILES_${PN}-x264": "${datadir}/gstreamer-1.0/presets/GstX264Enc.prs",
  "GSTREAMER_ORC": "orc\n",
  "PACKAGECONFIG[orc]": "-Dorc=enabled,-Dorc=disabled,orc orc-native",
  "GIR_MESON_ENABLE_FLAG": "enabled",
  "GIR_MESON_DISABLE_FLAG": "disabled",
  "PACKAGES_DYNAMIC": "^${PN}-.*"
