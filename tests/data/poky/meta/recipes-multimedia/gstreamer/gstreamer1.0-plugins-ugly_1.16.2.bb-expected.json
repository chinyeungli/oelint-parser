  "LICENSE": "GPLv2+ & LGPLv2.1+ & LGPLv2+",
  "LICENSE_FLAGS": "commercial",
  "SRC_URI": " \u001b            https://gstreamer.freedesktop.org/src/gst-plugins-ugly/gst-plugins-ugly-${PV}.tar.xz \u001b            ",
