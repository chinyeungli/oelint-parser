{
  "HOMEPAGE": "https://gstreamer.freedesktop.org/",
  "BUGTRACKER": "https://bugzilla.gnome.org/enter_bug.cgi?product=Gstreamer",
  "SECTION": "multimedia",
  "DEPENDS": "gstreamer1.0 glib-2.0-native\niso-codes util-linux zlib",
  "inherit": "gettext meson pkgconfig upstream-version-is-even\ngobject-introspection gtk-doc",
