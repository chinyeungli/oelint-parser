{
  "SUMMARY": "Xmu and Xmuu: X Miscellaneous Utility libraries",
  "DESCRIPTION": "The Xmu Library is a collection of miscellaneous (some \u001bmight say random) utility functions that have been useful in building \u001bvarious applications and widgets. This library is required by the Athena \u001bWidgets. A subset of the functions that do not rely on the Athena \u001bWidgets (libXaw) or X Toolkit Instrinsics (libXt) are provided in a \u001bsecond library, libXmuu.",
  "HOMEPAGE": "http://www.x.org",
  "BUGTRACKER": "https://bugs.freedesktop.org/enter_bug.cgi?product=xorg",
  "SECTION": "x11/libs",
  "LICENSE": "MIT-X\nMIT & MIT-style",
  "DEPENDS": "util-macros\nlibxt libxext",
  "XORG_PN": "${BPN}\nlibXmu",
  "SRC_URI": "${XORG_MIRROR}/individual/lib/${XORG_PN}-${PV}.tar.bz2",
  "S": "${WORKDIR}/${XORG_PN}-${PV}",
  "inherit": "autotools features_check pkgconfig",
  "EXTRA_OECONF": "--disable-specs --without-groff --without-ps2pdf --without-fop",
  "PACKAGECONFIG": "\n!!!inlineblock!!!",
  "PACKAGECONFIG[xmlto]": "--with-xmlto, --without-xmlto, xmlto-native docbook-xml-dtd4-native docbook-xsl-stylesheets-native",
  "UNKNOWN_CONFIGURE_WHITELIST": "--enable-malloc0returnsnull --disable-malloc0returnsnull \u001b                                --disable-specs --without-groff --without-ps2pdf --without-fop \u001b                                --without-xmlto --with-xmlto \u001b",
  "REQUIRED_DISTRO_FEATURES": "x11",
  "LIC_FILES_CHKSUM": "file://COPYING;md5=def3d8e4e9c42004f1941fa22f01dc18",
  "PROVIDES": "xmu",
  "PE": "1",
  "LEAD_SONAME": "libXmu",
  "PACKAGES": "libxmuu",
  "FILES": "${libdir}/libXmuu.so.*",
  "BBCLASSEXTEND": "native",
  "SRC_URI[md5sum]": "ac774cff8b493f566088a255dbf91201",
  "SRC_URI[sha256sum]": "9c343225e7c3dc0904f2122b562278da5fed639b1b5e880d25111561bac5b731",
  "PACKAGECONFIG[ipv6]": "--enable-ipv6,--disable-ipv6,"
}