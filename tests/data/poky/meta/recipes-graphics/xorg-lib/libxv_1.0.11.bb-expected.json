{
  "SUMMARY": "Xv: X Video extension library",
  "DESCRIPTION": "libXv provides an X Window System client interface to the \u001bX Video extension to the X protocol. The X Video extension allows for \u001baccelerated drawing of videos.  Hardware adaptors are exposed to \u001bclients, which may draw in a number of colourspaces, including YUV.",
  "HOMEPAGE": "http://www.x.org",
  "BUGTRACKER": "https://bugs.freedesktop.org/enter_bug.cgi?product=xorg",
  "SECTION": "x11/libs",
  "LICENSE": "MIT-X\nMIT-style",
  "DEPENDS": "util-macros\nlibxext xorgproto",
  "XORG_PN": "${BPN}\nlibXv",
  "SRC_URI": "${XORG_MIRROR}/individual/lib/${XORG_PN}-${PV}.tar.bz2",
  "S": "${WORKDIR}/${XORG_PN}-${PV}",
  "inherit": "autotools features_check pkgconfig",
  "EXTRA_OECONF": "--disable-specs --without-groff --without-ps2pdf --without-fop",
