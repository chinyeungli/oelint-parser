{
  "SUMMARY": "ICE: Inter-Client Exchange library",
  "DESCRIPTION": "The Inter-Client Exchange (ICE) protocol provides a \u001bgeneric framework for building protocols on top of reliable, byte-stream \u001btransport connections. It provides basic mechanisms for setting up and \u001bshutting down connections, for performing authentication, for \u001bnegotiating versions, and for reporting errors. ",
  "HOMEPAGE": "http://www.x.org",
  "BUGTRACKER": "https://bugs.freedesktop.org/enter_bug.cgi?product=xorg",
  "SECTION": "x11/libs",
  "LICENSE": "MIT-X\nMIT-style",
  "DEPENDS": "util-macros\nxorgproto xtrans",
  "XORG_PN": "${BPN}\nlibICE",
  "SRC_URI": "${XORG_MIRROR}/individual/lib/${XORG_PN}-${PV}.tar.bz2",
  "S": "${WORKDIR}/${XORG_PN}-${PV}",
  "inherit": "autotools features_check pkgconfig",
  "EXTRA_OECONF": "--disable-specs --without-groff --without-ps2pdf --without-fop",
  "PACKAGECONFIG": "\n!!!inlineblock!!!",
  "PACKAGECONFIG[xmlto]": "--with-xmlto, --without-xmlto, xmlto-native docbook-xml-dtd4-native docbook-xsl-stylesheets-native",
  "UNKNOWN_CONFIGURE_WHITELIST": "--enable-malloc0returnsnull --disable-malloc0returnsnull \u001b                                --disable-specs --without-groff --without-ps2pdf --without-fop \u001b                                --without-xmlto --with-xmlto \u001b",
  "REQUIRED_DISTRO_FEATURES": "x11",
  "LIC_FILES_CHKSUM": "file://COPYING;md5=d162b1b3c6fa812da9d804dcf8584a93",
  "PROVIDES": "ice",
  "PE": "1",
  "BBCLASSEXTEND": "native nativesdk",
  "SRC_URI[md5sum]": "76d77499ee7120a56566891ca2c0dbcf",
  "SRC_URI[sha256sum]": "6f86dce12cf4bcaf5c37dddd8b1b64ed2ddf1ef7b218f22b9942595fb747c348",
  "PACKAGECONFIG[arc4]": "ac_cv_lib_bsd_arc4random_buf=yes,ac_cv_lib_bsd_arc4random_buf=no,libbsd",
  "PACKAGECONFIG[ipv6]": "--enable-ipv6,--disable-ipv6,"
}