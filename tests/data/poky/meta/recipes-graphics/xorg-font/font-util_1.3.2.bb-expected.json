{
  "SUMMARY": "X.Org font package creation/installation utilities",
  "HOMEPAGE": "http://www.x.org",
  "BUGTRACKER": "https://bugs.freedesktop.org/enter_bug.cgi?product=xorg",
  "SECTION": "x11/fonts",
  "LICENSE": "MIT-X\nBSD & MIT",
  "DEPENDS": " encodings font-alias font-util-native\nencodings util-macros\nutil-macros-native",
  "RDEPENDS_${PN}": "encodings font-util font-alias\nmkfontdir mkfontscale encodings\n",
  "XORG_PN": "${BPN}",
