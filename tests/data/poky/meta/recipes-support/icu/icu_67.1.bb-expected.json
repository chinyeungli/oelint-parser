{
  "SUMMARY": "International Component for Unicode libraries",
  "DESCRIPTION": "The International Component for Unicode (ICU) is a mature, \u001bportable set of C/C++ and Java libraries for Unicode support, software \u001binternationalization (I18N) and globalization (G11N), giving applications the \u001bsame results on all platforms.",
  "HOMEPAGE": "http://site.icu-project.org/",
  "LICENSE": "ICU",
  "DEPENDS": "icu-native\n",
  "CVE_PRODUCT": "international_components_for_unicode",
  "S": "${WORKDIR}/icu/source",
  "SPDX_S": "${WORKDIR}/icu",
  "STAGING_ICU_DIR_NATIVE": "${STAGING_DATADIR_NATIVE}/${BPN}/${PV}",
  "BINCONFIG": "${bindir}/icu-config",
  "ICU_MAJOR_VER": "!!!inlineblock!!!",
  "inherit": "autotools pkgconfig binconfig multilib_script",
  "MULTILIB_SCRIPTS": "${PN}-dev:${bindir}/icu-config",
  "EXTRA_OECONF": "--with-cross-build=${STAGING_ICU_DIR_NATIVE}\n\n--with-cross-build=${STAGING_ICU_DIR_NATIVE}\n --with-data-packaging=archive\n ac_cv_func_strtod_l=no",
  "TARGET_CXXFLAGS": " -DICU_DATA_DIR=\u001b\u001b\"\"${datadir}/${BPN}/${PV}\u001b\u001b",
  "ASNEEDED": "",
  "PREPROCESS_RELOCATE_DIRS": "${datadir}/${BPN}/${PV}",
  "PACKAGES": "libicudata libicuuc libicui18n libicutu libicuio",
  "FILES_${PN}-dev": "${libdir}/${BPN}/",
  "FILES": "${libdir}/libicudata.so.*\n${libdir}/libicuuc.so.*\n${libdir}/libicui18n.so.*\n${libdir}/libicutu.so.*\n${libdir}/libicuio.so.*",
  "BBCLASSEXTEND": "native nativesdk",
  "LIC_FILES_CHKSUM": "file://../LICENSE;md5=a3808a5b70071b07f87ff2205e4d75a0",
  "ICU_PV": "!!!inlineblock!!!",
  "ICU_FOLDER": "!!!inlineblock!!!",
  "ARM_INSTRUCTION_SET": "arm\narm",
  "BASE_SRC_URI": "https://github.com/unicode-org/icu/releases/download/release-${ICU_FOLDER}/icu4c-${ICU_PV}-src.tgz",
  "DATA_SRC_URI": "https://github.com/unicode-org/icu/releases/download/release-${ICU_FOLDER}/icu4c-${ICU_PV}-data.zip",
  "SRC_URI": "${BASE_SRC_URI};name=code \u001b           ${DATA_SRC_URI};name=data \u001b           file://filter.json \u001b           file://fix-install-manx.patch \u001b           file://0001-icu-Added-armeb-support.patch \u001b           \n\u001b           file://0001-Disable-LDFLAGSICUDT-for-Linux.patch \u001b          ",
  "UPSTREAM_CHECK_REGEX": "icu4c-(?P<pver>\u001bd+(_\u001bd+)+)-src",
  "UPSTREAM_CHECK_URI": "https://github.com/unicode-org/icu/releases",
  "PACKAGECONFIG": "",
  "PACKAGECONFIG[make-icudata]": ",,,"
}